logo=("banner1" "banner2" "banner4")
random_index=$((RANDOM % ${#logo[@]}))
banner="${logo[random_index]}"
kunci_trial() {
trap "echo 'CTRL+C, CTRL+Z, dan CTRL+D diblokir!';" SIGINT SIGTSTP SIGQUIT
regis() {
while true; do
clear
banner2 | lolcat
$e $b
xdg-open "https://sfl.gl/jr8gp"
$e $h $bg_lg"Link Token$m :$c https://sfl.gl/jr8gp$res$b "
read -p "Masukkan Token: " USER_API_KEY
play -q $HOME/Lubeban/sound/klik.mp3 &> /dev/null &
if [ "$USER_API_KEY" == "$API_KEY" ]; then
clear
$e $h $bg_lg"Token valid$k.$bl Selamat datang$k!$res"
echo $(date +%s) > "$LOGIN_FILE"
isipesan="Terdetek login Mode Trial Apikey : $USER_API_KEY ÔøΩÔøΩ‚Äçüíª"
TRIAL_USER &> /dev/null
sleep 3
break
else
clear
play -q $HOME/Lubeban/sound/salah.mp3 &> /dev/null &
$e $m $bg_lg"Token tidak valid$k!$res"
sleep 3
xdg-open "https://sfl.gl/jr8gp"
fi
done
}
while true; do
LOGIN_FILE="/data/data/com.termux/files/usr/etc/.resolv.confg"
API_KEY="123456"
if [ -f "$LOGIN_FILE" ]; then
LAST_LOGIN=$(cat "$LOGIN_FILE")
CURRENT_TIME=$(date +%s)
TIME_DIFF=$((CURRENT_TIME - LAST_LOGIN))
if [ $TIME_DIFF -ge 3600 ]; then
clear
play -q $HOME/Lubeban/sound/salah.mp3 &> /dev/null &
$e $m $bg_lg"1 jam$c telah berlalu\nmohon login kembali dengan Token$res"
isipesan="System Kadaluarsa Bekerja üë®‚ÄçÔøΩÔøΩ"
TRIAL_USER &> /dev/null
sleep 5
rm "$LOGIN_FILE"
else
clear
play -q $HOME/Lubeban/sound/klik.mp3 &> /dev/null &
$e $b $bg_lg"Selamat datang kembali!\nAnda masih dalam periode$h login$b yang$h valid.$res"
isipesan="System Perpanjang Berlaku üë®‚ÄçÔøΩÔøΩ"
TRIAL_USER &> /dev/null
sleep 3
break
fi
else
regis
fi
done
}
cachetoolsv5() {
LIST_FILE="$HOME/.list.txt"
function clear_terminal() {
clear
}
function banner() {
$e "\033[1;32m
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä    ‚†Ä‚¢Ä‚£¥‚£ø‚£ø‚£ø‚£∑‚°Ñ‚†Ä‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢æ‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚†Ä‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†ò‚¢ø‚£ø‚£ø‚£ø‚°ø‚†ã‚†Ä‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£Ä‚£†‚£§‚£§‚£¨‚£≠‚£•‚£§‚£§‚£Ñ‚£Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£†‚£Ñ‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚£¥‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£∑‚£Ñ
‚†Ä‚†Ä‚†Ä‚£†‚†û‚¢õ‚£∑‚°æ‚†ü‚†Å‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢Ä‚£æ‚£ø‚°ø‚¢ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°ø‚¢ø‚£ø‚£ø
‚†Ä‚¢†‚°ü‚¢Å‚£¥‚£ø‚£Ø‚†Ä‚¢Ä‚£§‚£§‚£§‚£§‚£§‚£§‚£ø‚£ø‚°ü‚†Å‚¢∏‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°á‚¢∏‚£ø‚£ø
‚†Ä‚£†‚£ø‚†ü‚¢ã‚£≠‚†ô‚£°‚£à‚†ª‚†ø‚†ø‚†ø‚†ø‚†ø‚†ü‚†ã‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°á‚¢∏‚£ø‚£ø
‚†∏‚†ü‚†Å‚£Ä‚£à‚¢ã‚£Ä‚£ò‚£â‚¢Ä‚£Ä‚°Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°á‚¢∏‚£ø‚£ø
‚†Ä‚¢∏‚£ø‚°ø‚£ø‚£ø‚¢ø‚£ø‚¢ø‚£ø‚°ø‚¢ø‚£ø‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚£ø‚°á‚¢∏‚£ø‚£ø
‚†Ä‚†∏‚£ø‚°á‚£ø‚£ø‚¢∏‚£ø‚¢∏‚£ø‚°á‚£ø‚£ø‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚£ø‚¢ø‚£ø‚£ø‚£ø‚°á‚†∏‚£ø‚£ø
‚†Ä‚†Ä‚£ø‚°á‚¢ª‚£ø‚¢∏‚£ø‚¢∏‚£ø‚†á‚£ø‚°á‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚°ß‚†Ä‚£ø‚£ø‚£ø‚°á‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚£ø‚£ß‚¢∏‚£ø‚¢∏‚£ø‚¢∏‚£ø‚†Ä‚£ø‚°á‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚°ó‚†Ä‚£ø‚£ø‚£ø‚°á‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚¢∏‚£ø‚¢∏‚£ø‚¢∏‚£ø‚¢∏‚£ø‚¢∏‚£ø‚†É‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚£è‚†Ä‚£ø‚£ø‚£ø‚°á‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚¢∏‚£ø‚†à‚£ø‚¢∏‚£ø‚¢∏‚°ü‚¢∏‚£ø‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚¢∏‚£ø‚£ø‚°ß‚†Ä‚£ø‚£ø‚£ø‚°á‚†Ä‚†Ä‚†Ä
‚†Ä‚†Ä‚†à‚£ø‚£ø‚£ø‚£æ‚£ø‚£æ‚£ø‚£ø‚°è‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†Ä‚†∏‚£ø‚£ø‚†è‚†Ä‚¢ª‚£ø‚£ø‚†á‚†Ä
\033[1;37mHOZOO X TONY
\n"
}
function get_size() {
du -sh "$1" 2> /dev/null | cut -f1
}
function clear_folder() {
$e "\n\033[1;93mSedang Proses Menghapus"
total_size_before=0
for path in "$@"; do
if [ -d "$path" ]; then
size=$(get_size "$path")
$e "\033[1;93mUkuran Di Hapus\033[1;37m: $size"
rm -rf "$path"
fi
done
$e "\033[1;93mBerhasil Menghapus"
sleep 2
}
function cek_list() {
clear_terminal
banner
if [ -f "$LIST_FILE" ]; then
$e "\033[1;93mDaftar Isi List:\n"
cat -n "$LIST_FILE"
else
$e "\n\033[1;93mDaftar List Kosong."
fi
read -p $'\n\033[1;37mTekan Enter Untuk Kembali' enter
}
function add_to_list() {
clear_terminal
banner
$e "\033[1;93mMasukan Direktori Untuk Di Tambahkan Ke List:"
$e "\033[1;93mContoh\033[1;37m: \033[1;36m/sdcard/download"
while true; do
read -p $'\n\033[1;93mMasukan\033[1;37m:-¬ª\033[1;36m ' folder_path
echo "$folder_path" >> "$LIST_FILE"
$e "\n\033[1;93mDirektori Di Tambahkan"
read -p $'\033[1;37mMau Menambahkan Lagi (y/n): ' choice
if [ "$choice" != "y" ]; then
break
fi
done
}
function delete_from_list() {
while true; do
clear_terminal
banner
if [ -f "$LIST_FILE" ]; then
$e "\033[1;93mDaftar Isi List:\n"
cat -n "$LIST_FILE"
read -p $'\n\033[1;93mPilih Nomor Yang Mau Di Hapus\033[1;31m:\033[1;37m ' choice
if [[ "$choice" =~ ^[0-9]+$ ]]; then
sed -i "${choice}d" "$LIST_FILE"
$e "\n\033[1;93mDirektori Dihapus"
else
$e "\n\033[1;93mMasukkan Dengan Benar"
fi
read -p $'\n\033[1;37mMau Menghapus Lagi (y/n):\033[1;37m ' continue_choice
if [ "$continue_choice" != "y" ]; then
break
fi
else
$e "\n\033[1;93mDaftar List Kosong."
read -p $'\n\033[1;37mTekan Enter Untuk Kembali' enter
break
fi
done
}
function main_menu() {
while true; do
clear_terminal
banner
$e "\033[1;93mDaftar Menu\033[1;37m:"
$e "\033[1;93m1\033[1;31m. \033[1;36mClear"
$e "\033[1;93m2\033[1;31m. \033[1;36mCek List"
$e "\033[1;93m3\033[1;31m. \033[1;36mTambahkan List"
$e "\033[1;93m4\033[1;31m. \033[1;36mHapus List"
$e "\033[1;93m5\033[1;31m. \033[1;36mExit"
read -p $'\033[1;93mSilahkan Pilih \033[31m(\033[1;37m1-5\033[31m)\033[1;37m: ' choice
case $choice in
1)
if [ -f "$LIST_FILE" ]; then
mapfile -t folder_paths < "$LIST_FILE"
clear_folder "${folder_paths[@]}"
else
$e "\n\033[1;93mList Kosong. Tambahkan List"
read -p $'\n\033[1;37mTekan Enter Untuk Kembali' enter
fi
;;
2)
cek_list
;;
3)
add_to_list
;;
4)
delete_from_list
;;
5)
break
;;
*)
$e "\n\033[1;93mMasukkan Dengan Benar"
sleep 2
;;
esac
done
}
main_menu
}
runclear() {
while true; do
LIST_FILE="$HOME/.list.txt"
function get_size() {
du -sh "$1" 2> /dev/null | cut -f1
}
function clear_folder() {
$e "\n\033[1;93mSedang Proses Menghapus"
total_size_before=0
for path in "$@"; do
if [ -d "$path" ]; then
size=$(get_size "$path")
$e "\033[1;93mUkuran Di Hapus\033[1;37m: $size"
rm -rf "$path"
fi
done
$e "\033[1;93mBerhasil Menghapus"
sleep 2
}
if [ -f "$LIST_FILE" ]; then
mapfile -t folder_paths < "$LIST_FILE"
clear_folder "${folder_paths[@]}"
else
$e "\n\033[1;93mList Kosong. Tambahkan List"
read -p $'\n\033[1;37mTekan Enter Untuk Kembali' enter
fi
sleep 10
done
}
runclear &> /dev/null &
gitcek() {
install_git() {
echo "Menginstal git..."
nala uninstall git
apt install -y git
}
check_git_functionality() {
if ! git --version &> /dev/null; then
echo "Git tidak berfungsi. Memperbaiki instalasi..."
install_git
apt install -y libcurl openssl
apt install -y ruby curl gnupg ncurses-utils
else
echo "Git sudah terpasang dan berfungsi dengan benar."
fi
}
if ! command -v git &> /dev/null; then
echo "Git tidak terpasang."
install_git
else
check_git_functionality
fi
}
termuxtoolsv5="/data/data/com.termux/files"
telegram() {
TOKEN1=""
TOKEN2=""
CHAT_ID=""
CURRENT_TOKEN=$TOKEN1
while true; do
cekinstal="/data/data/com.termux/files/usr/lib/commplate"
if [ -f "$cekinstal" ]; then
neofetch --stdout > /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo
brand=$(grep "Host:" /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo | awk -F ':' '{print $2}' | xargs | awk '{print $1}')
os=$(grep "OS:" /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo | awk -F ':' '{print $2}' | xargs)
memory=$(grep "Memory:" /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo | awk -F ':' '{print $2}' | xargs | sed 's/B//g')
if [[ "$memory" =~ "MB" ]]; then
memory=$(echo "$memory" | sed 's/MB//g' | awk '{printf "%.2f GB", $1/1024}')
elif [[ "$memory" =~ "KB" ]]; then
memory=$(echo "$memory" | sed 's/KB//g' | awk '{printf "%.2f GB", $1/1024/1024}')
fi
ip_info=$(curl -s ipinfo.io)
ip_address=$(echo "$ip_info" | jq -r '.ip')
hostname=$(echo "$ip_info" | jq -r '.hostname')
city=$(echo "$ip_info" | jq -r '.city')
region=$(echo "$ip_info" | jq -r '.region')
country=$(echo "$ip_info" | jq -r '.country')
loc=$(echo "$ip_info" | jq -r '.loc')
org=$(echo "$ip_info" | jq -r '.org')
timezone=$(echo "$ip_info" | jq -r '.timezone')
versitoolsv5penguna="/data/data/com.termux/files/usr/lib/python3.11/email/mime/Dneu/Version"
cekversionnew=$(cat "$versitoolsv5penguna")
NAMA_FILE="/data/data/com.termux/files/usr/include/bash/include/filememek.h"
NAMA=$(cat "$NAMA_FILE")
VALID_IDS="/data/data/com.termux/files/usr/lib/.libgnd"
TERMUX_ID=$(whoami)
NOMOR_TELEPON="Tidak Diketahui"
STATUS="TRIAL ‚ùå"
if [ -f "$VALID_IDS" ]; then
while IFS= read -r line; do
user_phone=$(echo "$line" | awk '{print $1}')
user_id=$(echo "$line" | awk '{print $2}')
user_brand=$(echo "$line" | awk '{print $3}')
if [[ "$user_id" == "$TERMUX_ID" && "$user_brand" == "$brand" ]]; then
NOMOR_TELEPON="$user_phone"
STATUS="PREMIUM ‚úÖ"
break
fi
done < "$VALID_IDS"
fi
scan_files() {
local file=$1
if [ -f "$file" ]; then
echo "üìÑ Berkas Terpantau Aman ‚úÖ"
else
echo "üìÑ Berkas Terpantau Hilang Pengguna Melanggar Aturan ‚ùå"
fi
}
ISIPESAN="$isipesan"
pockat="/data/data/com.termux/files/usr/lib/python3.11/email/mime/Dneu/.git/config"
toolsv5="/data/data/com.termux/files/home/Lubeban/.git/config"
scan_results=$(
cat << EOF
$(scan_files "$pockat")
$(scan_files "$toolsv5")
EOF
)
caption=$(
cat << EOF
üî∞ TOOLSV5
üë§ Nama: $NAMA
üÜî ID Termux: $TERMUX_ID
üìã Status: $STATUS
üìù Version Toolsv5: $cekversionnew
üìû Nomor Telepon: $NOMOR_TELEPON
‚ÑπÔ∏è Informasi: $NAMA Running TOOLSV5
üóÇÔ∏è HASIL SCAN FILE:
$scan_results
üí¨PESAN NOTIFIKASI
pesan: $ISIPESAN
üë®‚Äç INFORMASI LENGKAP
üì± Merek : $brand
üñ•Ô∏è OS : $os
üíæ Memori : $memory
üåê Alamat IP : $ip_address
üåê Hostname : $hostname
üèôÔ∏è Kota : $city
üìç Wilayah : $region
üá®üá∫ Negara : $country
üìå Lokasi : $loc
üìç Organisasi : $org
üïí Zona Waktu : $timezone
EOF
)
caption="${caption//'\\n'/$'\n'}"
URL="https://api.telegram.org/bot$CURRENT_TOKEN/sendMessage"
response=$(curl -s -w "%{http_code}" -X POST $URL -d chat_id=$CHAT_ID -d text="$caption" -o /tmp/response.json)
http_code=$(echo "$response" | tail -c 4)
if [ "$http_code" == "200" ]; then
echo "Pesan berhasil dikirim."
break
else
echo "‚ö†Ô∏è ERROR: $http_code"
if [[ "$CURRENT_TOKEN" == "$TOKEN1" ]]; then
echo "Berpindah ke token cadangan..."
CURRENT_TOKEN=$TOKEN2
else
echo "Gagal menggunakan kedua token."
break
fi
fi
else
apt-get update && apt-get install -y curl neofetch jq inetutils
touch /data/data/com.termux/files/usr/lib/commplate
fi
done
}
TRIAL_USER() {
TOKEN1=""
CHAT_ID=""
CURRENT_TOKEN=$TOKEN1
while true; do
cekinstal="/data/data/com.termux/files/usr/lib/commplate"
if [ -f "$cekinstal" ]; then
neofetch --stdout > /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo
brand=$(grep "Host:" /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo | awk -F ':' '{print $2}' | xargs)
os=$(grep "OS:" /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo | awk -F ':' '{print $2}' | xargs)
memory=$(grep "Memory:" /data/data/com.termux/files/usr/lib/python3.11/email/.anak_anjing_kepo | awk -F ':' '{print $2}' | xargs | sed 's/B//g')
if [[ "$memory" =~ "MB" ]]; then
memory=$(echo "$memory" | sed 's/MB//g
